37a:["$","p",null,{"children":["As progressing time is a common requirement, the ",["$","code",null,{"children":"fake"}]," method accepts a ",["$","code",null,{"children":"syncWithCarbon"}]," argument to keep Carbon in sync when sleeping within a test:"]}]
