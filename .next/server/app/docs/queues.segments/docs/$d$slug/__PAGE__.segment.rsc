6a:["$","pre",null,{"children":["$","code",null,{"className":"language-php","children":"use Illuminate\\Support\\Facades\\Redis;\n\n/**\n * Execute the job.\n */\npublic function handle(): void\n{\n    Redis::throttle('key')->block(0)->allow(1)->every(5)->then(function () {\n        info('Lock obtained...');\n\n        // Handle job...\n    }, function () {\n        // Could not obtain lock...\n\n        return $this->release(5);\n    });\n}\n"}]}]
